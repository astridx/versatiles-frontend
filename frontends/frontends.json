{
	"frontend": {
		"include": [
			"all",
			"frontend",
			"mapdesigner"
		],
		"dev": {
			"proxy": [
				{
					"from": "/tiles/",
					"to": "https://tiles.versatiles.org/tiles/"
				}
			]
		}
	},
	"frontend-rust": {
		"include": [
			"all",
			"frontend-rust",
			"mapdesigner"
		]
	},
	"frontend-minimal": {
		"include": [
			"all",
			"frontend-minimal"
		],
		"ignore": [
			"assets/fonts/",
			"!assets/fonts/noto*"
		]
	}
}